	public static void main(String[] args) {

		System.out.println("1 for retrive data");
		System.out.println("1 for insert data");
		System.out.println("1 for delete data");
		System.out.println("1 for search data");
		System.out.println("1 for update data");

		Scanner sc = new Scanner(System.in);
		int ch=sc.nextInt();
		switch(ch){
		case 1:
			break;
		case 2:
			break;
		case 3:
			break;
		case 4:
			break;
		case 5:
			break;



		}
	}
}
